#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FREESCOUT_STORAGE doesn't exist, the content setup has not worked correctly.
#======================================================================================================================

[[ ! -d ${FREESCOUT_STORAGE} ]] && bf-error "${FREESCOUT_STORAGE} does not exist."


#======================================================================================================================
# Run setup config executable.
#======================================================================================================================

${FREESCOUT_LIB}/setup-config
