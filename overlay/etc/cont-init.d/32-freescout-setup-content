#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FREESCOUT_SRC doesn't exist, the source files have not been downloaded correctly.
#======================================================================================================================

[[ ! -d ${FREESCOUT_SRC} ]] && bf-error "FreeScout source files cannot be found."


#======================================================================================================================
# Run setup content executable.
#======================================================================================================================

${FREESCOUT_LIB}/setup-content
