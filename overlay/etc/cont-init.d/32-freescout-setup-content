#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FREESCOUT_SRC doesn't exist, the setup has gone wrong.
#======================================================================================================================

[[ ! -f ${FREESCOUT_SRC} ]] && bf-error "${FREESCOUT_SRC} does not exist."


#======================================================================================================================
# Run setup content executable.
#======================================================================================================================

${FREESCOUT_LIB}/setup-content
